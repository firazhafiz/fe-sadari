generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id            Int       @id @default(autoincrement())
  nama          String
  alamat        String?
  tanggal_lahir DateTime?
  no_hp         String?
  answers       Answer[]
}

model Answer {
  id           Int   @id @default(autoincrement())
  userId       Int
  jawaban1     Int?
  jawaban2     Int?
  jawaban3     Int?
  jawaban4     Int?
  jawaban5     Int?
  jawaban6     Int?
  jawaban7     Int?
  jawaban8     Int?
  jawaban9     Int?
  jawaban10    Int?
  jawaban11    Int?
  jawaban12    Int?
  jawaban13    Int?
  jawaban14    Int?
  jawaban15    Int?
  jawaban16    Int?
  jawaban17    Int?
  jawaban18    Int?
  jawaban19    Int?
  jawaban20    Int?
  hasil_persen Float
  user         User  @relation(fields: [userId], references: [id], onDelete: Cascade)
}
